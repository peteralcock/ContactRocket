<p id="notice"><%= notice %></p>

<h1>Listing Bots</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Name</th>
      <th>Bot type</th>
      <th>Dict file</th>
      <th>Aiml</th>
      <th>Brain</th>
      <th>Bot iq</th>
      <th>Brain speed</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bots.each do |bot| %>
      <tr>
        <td><%= bot.user_id %></td>
        <td><%= bot.name %></td>
        <td><%= bot.bot_type %></td>
        <td><%= bot.dict_file %></td>
        <td><%= bot.aiml_id %></td>
        <td><%= bot.brain_id %></td>
        <td><%= bot.bot_iq %></td>
        <td><%= bot.brain_speed %></td>
        <td><%= link_to 'Show', bot %></td>
        <td><%= link_to 'Edit', edit_bot_path(bot) %></td>
        <td><%= link_to 'Destroy', bot, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Bot', new_bot_path %>
